<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Video"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[shrss.video.v1.editor]"
    trackingFeature="core-components:embed:v2">
    <content
        granite:class="cmp-embed__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <damtext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                level="{Long}5"
                                                text="Note : For Dynamic Media Video, Please drag and drop dynamic media enabled video."/>
                                            <type
                                                granite:class="cmp-embed-dialog-edit__type"
                                                granite:hide="${(cqDesign.externalUrlDisabled &amp;&amp; cqDesign.embeddablesDisabled)}"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                deleteHint="{Boolean}true"
                                                fieldLabel="Type"
                                                name="./type"
                                                vertical="{Boolean}false">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cmp-embed-dialog-edit-hook="typeField"
                                                    cmp-embed-dialog-edit-showhidetarget="\[data-cmp-embed-dialog-edit-type]"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <url
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:hideResource="{Boolean}true"/>
                                                    <externalurl
                                                        granite:hide="${cqDesign.externalUrlDisabled}"
                                                        jcr:primaryType="nt:unstructured"
                                                        checked="{Boolean}true"
                                                        text="External URL"
                                                        value="externalUrl"/>
                                                    <embeddable
                                                        granite:hide="${cqDesign.embeddablesDisabled}"
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Third Party"
                                                        value="embeddable"/>
                                                    <html
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:hideResource="{Boolean}true"/>
                                                </items>
                                            </type>
                                            <url
                                                jcr:primaryType="nt:unstructured"
                                                sling:hideResource="{Boolean}true"/>
                                            <externalUrl
                                                granite:class="foundation-toggleable"
                                                granite:hide="${cqDesign.externalUrlDisabled}"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        emptyText="Paste a URL to embeddable content"
                                                        fieldDescription="The URL of the content to embed. The URL will be checked against supported processors for a match."
                                                        fieldLabel="External URL"
                                                        name="./externalUrl"
                                                        required="{Boolean}true">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cmp-embed-dialog-edit-hook="externalUrlField"/>
                                                    </field>
                                                    <width
		                                                jcr:primaryType="nt:unstructured"
		                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
		                                                fieldDescription="Width to apply to the component."
		                                                fieldLabel="Width"
		                                                name="./externalWidth">
		                                                <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cmp-embed-dialog-edit-hook="externalUrlField"/>
                                                     </width>
                                                    <status
                                                        granite:class="coral-Form-fieldlabel foundation-toggleable"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                                        text="">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cmp-embed-dialog-edit-hook="externalUrlStatus"/>
                                                    </status>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cmp-embed-dialog-edit-type="true"
                                                    cmp-embed-dialog-edit-showhidetargetvalue="externalUrl"/>
                                            </externalUrl>
                                            <embeddable
                                                granite:class="foundation-toggleable"
                                                granite:hide="${cqDesign.embeddablesDisabled}"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        deleteHint="{Boolean}true"
                                                        fieldDescription="Select the thrid party video to include on the page. Options for the third party are shown once selected."
                                                        fieldLabel="Third Party"
                                                        name="./embeddableResourceType">
                                                        <datasource
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="core/wcm/components/embed/v2/datasources/allowedembeddables"/>
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cmp-embed-dialog-edit-hook="embeddableField"
                                                            cmp-embed-dialog-edit-showhidetarget="\[data-cmp-embed-dialog-edit-embeddableoptions]"/>
                                                    </field>
                                                    <options
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                        <datasource
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="core/wcm/components/embed/v2/datasources/embeddableoptions"/>
                                                    </options>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cmp-embed-dialog-edit-showhidetargetvalue="embeddable"
                                                    cmp-embed-dialog-edit-type="true"/>
                                            </embeddable>
                                            <html
                                                jcr:primaryType="nt:unstructured"
                                                sling:hideResource="{Boolean}true"/>
                                            <class
				                                jcr:primaryType="nt:unstructured"
				                                sling:resourceType="granite/ui/components/foundation/include"
				                                path="shrss/components/commonfields/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/class"/>
				                            <id
				                                jcr:primaryType="nt:unstructured"
				                                sling:resourceType="granite/ui/components/foundation/include"
				                                path="shrss/components/commonfields/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/id"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
