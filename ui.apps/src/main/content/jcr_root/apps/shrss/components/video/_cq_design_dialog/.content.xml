<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Video"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[shrss.video.v1.editor]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container"
        granite:class="cmp-embed__design-editor">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <datasource
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core/wcm/components/embed/v2/datasources/embeddesigntabs"/>
                <firsttabs jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Embeddable Types"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <urlDisabled
                                        jcr:primaryType="nt:unstructured"
                                        sling:hideResource="{Boolean}true"/>
                                    <externalUrlDisabled
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        checked="false"
                                        fieldDescription="When checked, disables the ability of content authors to enter 3D URL for rendering a widget."
                                        name="./externalUrlDisabled"
                                        text="Disable External URL"
                                        uncheckedValue="false"
                                        value="{Boolean}true"/>
                                    <embeddablesDisabled
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        checked="false"
                                        fieldDescription="When checked, disables the ability of content authors to select an embeddable from a list of allowed embeddables."
                                        name="./embeddablesDisabled"
                                        text="Disable Third Party"
                                        uncheckedValue="false"
                                        value="{Boolean}true"/>
                                    <allowedEmbeddables
                                        granite:class="allowed-embeddables"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        deleteHint="{Boolean}true"
                                        fieldDescription="The embeddables that are allowed to be selected by an author when embeddables are not disabled."
                                        fieldLabel="Allowed Embeddables"
                                        multiple="{Boolean}true"
                                        name="./allowedEmbeddables">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="core/wcm/components/embed/v2/datasources/embeddables"/>
                                    </allowedEmbeddables>
                                    <htmlDisabled
                                        jcr:primaryType="nt:unstructured"
                                        sling:hideResource="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </properties>
                 </firsttabs>
            </tabs>
        </items>
    </content>
</jcr:root>
